# Supabase / Database
# Connect to local postgres container
DATABASE_URL="postgresql://postgres:postgres@postgres:5432/sinesys"
POSTGRES_PASSWORD="postgres"

# Redis
REDIS_URL="redis://redis:6379"
ENABLE_REDIS_CACHE="true"

# Supabase Client (Public)
# Note: If running only Postgres+Studio locally, Supabase Auth/Realtime/Storage APIs won't be available.
# You might need to point these to a hosted Supabase project or add the other services to docker-compose.
NEXT_PUBLIC_SUPABASE_URL="http://localhost:54321"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_OR_ANON_KEY="your-anon-key-here"

# Supabase Admin (Secret)
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"
SUPABASE_SECRET_KEY="your-service-role-key-here"

# Feature Flags
ENABLE_AI_INDEXING="false"

# Local Browser Service (if needed)
BROWSER_WS_ENDPOINT=""
BROWSER_SERVICE_URL=""

# Mailhog
SMTP_HOST="mailhog"
SMTP_PORT="1025"
