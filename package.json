    "validate:build-performance": "node scripts/validate-build-performance.js",
    "validate:build-performance:strict": "node scripts/validate-build-performance.js --strict",
    "validate:build-performance:warn": "node scripts/validate-build-performance.js --warn",
    "validate:cache": "node scripts/validate-build-cache.js",
