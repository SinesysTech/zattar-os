    "dev": "next dev",
    "dev:verbose": "next dev",
    "dev:no-deprecation": "NODE_OPTIONS=--no-deprecation next dev",
    "dev:trace": "node --trace-deprecation node_modules/next/dist/bin/next dev",
    "build": "next build --turbopack",
    "build:verbose": "next build --turbopack",
    "build:no-deprecation": "NODE_OPTIONS=--no-deprecation next build --turbopack",
    "build:prod": "next build --webpack",
    "build:prod:webpack": "next build --webpack",
    "build:prod:turbopack": "next build --turbopack",
    "build:caprover": "NODE_OPTIONS='--max-old-space-size=6144' next build --webpack",
    "analyze": "node scripts/run-analyze.js",
    "build:debug-memory": "node scripts/run-build-debug-memory.js",
    "check:memory": "node -e \"const cp=require('child_process'); const hasBash=cp.spawnSync('bash',['--version'],{stdio:'ignore'}).status===0; if(!hasBash){console.error('check:memory requires a POSIX shell (WSL or Git Bash on Windows).'); process.exit(1);} const r=cp.spawnSync('bash',['scripts/check-build-memory.sh'],{stdio:'inherit'}); process.exit(r.status==null?1:r.status);\"",
    "prebuild:check": "node -e \"const cp=require('child_process'); const hasBash=cp.spawnSync('bash',['--version'],{stdio:'ignore'}).status===0; if(!hasBash){console.warn('prebuild:check: POSIX shell not found; skipping memory check.'); process.exit(0);} cp.spawnSync('bash',['scripts/check-build-memory.sh'],{stdio:'inherit'}); process.exit(0);\"",
    "check:pwa": "node scripts/check-pwa.js",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "type-check:skip-lib": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:unit": "jest --testMatch='**/*.test.ts' --testPathIgnorePatterns='integration'",
    "test:api-acervo-geral": "tsx dev_data/scripts/test-api-acervo-geral.ts",
    "test:api-arquivados": "tsx dev_data/scripts/test-api-arquivados.ts",
