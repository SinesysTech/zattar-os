    "build": "npm run check:architecture && node --max-old-space-size=6144 node_modules/next/dist/bin/next build --experimental-build-mode=compile",
    "build:ci": "npm run check:architecture && node --max-old-space-size=6144 node_modules/next/dist/bin/next build --experimental-build-mode=compile",
