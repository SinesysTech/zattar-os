    "build:debug-memory": "node scripts/run-build-debug-memory.js",
    "check:memory": "node -e \"const cp=require('child_process'); const hasBash=cp.spawnSync('bash',['--version'],{stdio:'ignore'}).status===0; if(!hasBash){console.error('check:memory requires a POSIX shell (WSL or Git Bash on Windows).'); process.exit(1);} const r=cp.spawnSync('bash',['scripts/check-build-memory.sh'],{stdio:'inherit'}); process.exit(r.status==null?1:r.status);\"",
    "prebuild:check": "node -e \"const cp=require('child_process'); const hasBash=cp.spawnSync('bash',['--version'],{stdio:'ignore'}).status===0; if(!hasBash){console.warn('prebuild:check: POSIX shell not found; skipping memory check.'); process.exit(0);} cp.spawnSync('bash',['scripts/check-build-memory.sh'],{stdio:'inherit'}); process.exit(0);\"",
